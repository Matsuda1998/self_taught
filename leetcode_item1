from typing import Optional
class Solution:
    def twoSum(self, nums: list, target: int) -> list:
        ans_list=[]
        for j,val in enumerate(nums) :
            for i in range(j+1,len(nums)):
                if val+nums[i] == target:
                    ans_list.append(j)
                    ans_list.append(i)
                    return ans_list
        print("nums is invalid list!")


Solution().twoSum([1,7,8,11],9)


